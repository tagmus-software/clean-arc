{
  "name": "typescript-backend-example",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start:dev": "tsx watch src/main.ts",
    "build": "tsc --project tsconfig.json",
    "start": "tsx dist/main.js",
    "typeorm:migration:create": "typeorm-ts-node-esm migration:create ",
    "typeorm:migration:up": "typeorm-ts-node-esm migration:run -d ./src/infra/database/mysql/ormconfig.ts",
    "typeorm:migration:down": "typeorm-ts-node-esm migration:revert -d ./src/infra/database/mysql/ormconfig.ts",
    "typeorm:seed": "tsx src/infra/database/mysql/seeders/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "mysql2": "^3.1.2",
    "pino": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.3",
    "typescript": "^4.4.4"
  }
}
